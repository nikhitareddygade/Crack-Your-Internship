class Solution {
    public String reverseWords(String s) {
        String[] str = s.trim().split("\\s+");
        StringBuilder res = new StringBuilder();
        for(int i=str.length-1;i>=0;i--){
            if(i == 0){
                res.append(str[i]);
            }
            else{
                res.append(str[i]);
                res.append(' ');
            }
        }
        return res.toString();
    }
}
